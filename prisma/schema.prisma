datasource db {
    provider = "sqlite"
    url      = "file:../data/dataBase.db"
}

generator client {
    provider = "prisma-client-js"
}

model Codes {
    id      Int      @id @default(autoincrement())
    code    String
    expires DateTime

    telegram_id   String
    first_name    String?
    last_name     String?
    username      String?
    language_code String?
}
